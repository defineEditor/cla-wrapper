{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["src/claWrapper.js", "README.md"]
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/"
    },
    "docdash": {
        "search": true,
        "collapse": true
    },
    "templates": {
        "default": {
            "includeDate": false
        }
    }
}
